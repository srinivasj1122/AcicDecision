package com.unisys.leida.ebis.sharedobjects.server.fact;

import java.lang.Number;

rule "AcicDecisionRule"
	dialect "mvel"
	ruleflow-group "acicDecision"
	salience 10
	when
		ad : ACICDecision( nationality == "ARGE" || nationality == "ARME" || nationality == "AUST" || nationality == "BELM" || nationality == "BRAZ" , gender == "M" || gender == "F" , age >= 18 )
	then
		ad.setResult( "1" );
end
